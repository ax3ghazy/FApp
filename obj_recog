import boto3

Input = input("image searching for ")

client = boto3.client('s3')


client.upload_file(Input, 'ai5', Input)

rekog = boto3.client('rekognition')
response = rekog.detect_labels(
    Image={
        'S3Object': {
            'Bucket': 'ai5',
            'Name': Input
        }
    },
    MaxLabels=123,
)

print(response, '\n')

print ("Most probably you're looking at ")

for match in response['Labels']:
    #if(match['Confidence'] > 80):
        print(match['Name'], ' ')



